{
    "extends": ["stylelint-config-standard"] ,
    "rules": {
        "selector-class-pattern": null,
        "property-no-vendor-prefix": [
            true,
            {
                "severity": "warning",
                "disableFix": true
            }
        ],
        "value-keyword-case": null,
        "no-duplicate-selectors": [
            true,
            {
                "severity": "warning",
                "disableFix": true
            }
        ],
        "comment-empty-line-before": null
    }
}
